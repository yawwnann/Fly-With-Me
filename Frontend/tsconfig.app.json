{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": [
    "src/**/__tests__/*",
    "src/views/admin/components/plugins",
    "src/views/admin/components/icon",
    "src/views/admin/views",
    "src/views/admin/layouts/auth-layout.vue"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
